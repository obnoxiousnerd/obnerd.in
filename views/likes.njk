---
title: Likes
---

<h1>{{ title }}</h1>

{% for like in likes.likes %}
    <section class="post-preview h-entry">
        <p>
            <a class="p-name" href="{{ like.url }}">{{ like.url }}</a>
        </p>
        <small>
            <time class="dt-published" datetime="{{ like.date | dateToRfc3339 }}">{{ like.date | dateToHuman }}</time>
            <br>
            <a class="u-url" href="/likes/{{ like.url | contentHash }}">permalink</a>
        </small>
    </section>
{% endfor %}