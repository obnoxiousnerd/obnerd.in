---
layout: main
---
<article class="h-entry">
  <header>
    <h1 class="p-name" class="title">{{ title }}</h1>
    <time class="dt-published" datetime="{{ date | dateToRfc3339 }}">
      {{ date | dateToHuman }}
    </time>
    <span class="seperator">&centerdot;</span>
    <span>By</span>{% include "thingmejacks/h-card-mini.njk" %}
  </header>
  <aside>
    {{ content | toc | safe }}
  </aside>
  <hr/>
  <div class="e-content">
    {{ content | safe }}
  </div>
</article>